mapscripts MODHeadquarters_General_Store_MapScripts {
    MAP_SCRIPT_ON_TRANSITION: GeneralStore_OnTransition
}

script GeneralStore_OnTransition{
    if(var(VAR_MOD_SHOPS_STATE) == 1101 || 
    var(VAR_MOD_SHOPS_STATE) == 101 || var(VAR_MOD_SHOPS_STATE) == 1001 || var(VAR_MOD_SHOPS_STATE) == 1100 ||
    var(VAR_MOD_SHOPS_STATE) == 1 || var(VAR_MOD_SHOPS_STATE) == 100 || var(VAR_MOD_SHOPS_STATE) == 1000 ||
    var(VAR_MOD_SHOPS_STATE) == 0)
    {
        addvar(VAR_MOD_SHOPS_STATE, 10)
    }
    if(var(VAR_MOD_SHOPS_STATE) == 0)
    {
        clearflag(FLAG_HIDE_DENEITH)
    }
}

script GeneratlStore_ObjectScript_MartEmployer
{
    lockall
    faceplayer
    if(var(VAR_MOD_SHOPS_STATE) < 2000)
    {
        msgbox(format("Welcome to my humble shop!\p
        Pokeballs, potions, held items, repels and more!\p
        If your looking for anything come check with me, because I will most likely have it somewhere around!\p
        That being said... {COLOR LIGHT_GRAY}The Leader{COLOR DARK_GRAY} does restrict me to sell... 'good' items to low level Initiatives.\p
        The higher your rank will become, the more items I'm allowed to sell you, so please do consider to come back from time to time!\p
        With that being said..."))
    }
    if(!flag(FLAG_GIFT_GENERALSTORE))
    {
        msgbox(format("Let me give you some things on the house this once, to get you started on your journey.\p
        I would say don't waste it, but if you do you're more quickly to come back to me so...\l
        do waste it I guess?"))
        giveitem(ITEM_POKE_BALL, 15)
        giveitem(ITEM_POTION, 5)
        setflag(FLAG_GIFT_GENERALSTORE)
        msgbox("Hope you will come back for more!")
    }
    msgbox(format("How can I be of service to you today?"))
    switch(var(VAR_BADGE_AMOUNT))
    {
        case 0:
            pokemart(ModHeadquarters_MartNoBadges)
        case 1:
            pokemart(ModHeadquarters_MartOne)
        case 2:
            pokemart(ModHeadquarters_MartTwo)
        case 3:
            pokemart(ModHeadquarters_MartThree)
        case 4:
            pokemart(ModHeadquarters_MartFour)
        case 5:
            pokemart(ModHeadquarters_MartFive)
        case 6:
            pokemart(ModHeadquarters_MartSix)
        case 7:
            pokemart(ModHeadquarters_MartSeven)
        case 8:
            pokemart(ModHeadquarters_MartEight)
        default:
            msgbox(format("Oh, it seems like something went wrong, please come again later!"))
    }
    msgbox(format("Please come again later!"))
    releaseall
}

